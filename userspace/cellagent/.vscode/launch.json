{
    "version": "2.0.0",
    "configurations": [
      {
        "name": "Launch simulator",
        "type": "lldb",
        "request": "launch",
        "args": [
            "${workspaceFolder}/configs/10cell_config.json"       
        ],
        "program": "${workspaceFolder}/target/debug/simulator",
        "windows": {
          "program": "${workspaceFolder}/target/debug/simulator.exe"
        },
        "cwd": "${workspaceFolder}",
        "stopOnEntry": false,
        "sourceLanguages": [
          "rust"
        ],
        "env":{"SERVER_URL":"127.0.0.1:8088", "HTML": "html/index.html"},
        //"preLaunchTask": "cargo build"
      }
    ]
}